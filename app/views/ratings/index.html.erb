<p style="color: green"><%= notice %></p>
<h2>List of ratings</h2>
<%= link_to "Create new rating", new_rating_path %>

<div id="ratings">
  <% u = User.first %>
  <% Rating.all.each{ |r| u.ratings << r } %>
<ul>
  <% @ratings.each do |rating| %>
    <li> <%= render rating %> 
    Made by: 
    <%= link_to rating.user.username, rating.user %> 
    </li>
  <% end %>
</ul>
 
</div>